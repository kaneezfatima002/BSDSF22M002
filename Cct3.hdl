CHIP Cct3{
	IN w,x,y,z;
	OUT out1;
	PARTS:
		Not(in=y, out=Noty);
		Not(in=z, out=Notz);
		And(a=w, b=Noty, out=temp1);
		And(a=Noty, b=Notz, out=temp2);
		And(a=x, b=temp2, out=temp3);
		And(a=x, b=Notz, out=temp4);
		And(a=w, b=temp4, out=temp5);
		Or(a=temp1, b=temp3, out=temp6);
		Or(a=temp5, b=temp6, out=out1);
}